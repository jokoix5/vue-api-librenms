A,CAAA,KAAA,OAAA,CAAA,CAAA;AACA,UAAA,KAAA,mBAAA,GAAA,CAAA;AACA,UAAA,MAAA,oBAAA,GAAA,CAAA;AACA,UAAA,GAAA,GAAA,CAAA,OAAA,EAAA,QAAA,EAAA,MAAA,EAAA,KAAA,EAAA,MAAA,EAAA,IAAA,CAAA,GAAA,CAAA;AAEA,WAAA,KAAA,CACA,GADA,CACA,GADA,EACA;AAAA,QAAA,YAAA,EAAA;AAAA,OADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA,SAHA,MAGA;AACA,cAAA,MAAA,GAAA,IAAA,UAAA,EAAA;AAEA,UAAA,MAAA,CAAA,aAAA,CAAA,QAAA,CAAA,IAAA;;AAEA,UAAA,MAAA,CAAA,MAAA,GAAA,YAAA;AACA,YAAA,KAAA,CAAA,WAAA,GAAA,MAAA,CAAA,MAAA;AACA,WAFA;;AAIA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA;AACA,OAlBA,EAmBA,KAnBA,CAmBA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA;;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA,OAtBA;AAuBA;AAjCA;AA3BA,CAAA","sourcesContent":["<template>\n  <v-card flat class=\"mx-auto\">\n    <div v-if=\"isEmpty\">\n      <v-alert type=\"warning\">\n        Opps! Graph for Uptime is not available.\n      </v-alert>\n    </div>\n\n    <div v-if=\"isLoading\">\n      <Loading />\n    </div>\n\n    <div v-if=\"!isLoading\">\n      <div v-if=\"!isEmpty\">\n        <v-img\n          class=\"white--text align-end\"\n          max-height=\"700px\"\n          max-width=\"100%\"\n          :src=\"uptimeImage\"\n        ></v-img>\n      </div>\n    </div>\n  </v-card>\n</template>\n\n<script>\nimport Loading from \"@/components/Loading\"\nimport { toTimestamp } from \"../utils/date\"\n\nexport default {\n  name: \"Uptime\",\n\n  components: { Loading },\n\n  props: [\"dateStart\", \"dateEnd\"],\n\n  data: () => ({\n    uptimeImage: \"\",\n    isEmpty: false,\n    isLoading: false,\n  }),\n\n  mounted() {\n    this.fetchUptimeGraph()\n  },\n\n  watch: {\n    dateStart() {\n      this.fetchUptimeGraph()\n    },\n\n    dateEnd() {\n      this.fetchUptimeGraph()\n    },\n  },\n\n  methods: {\n    fetchUptimeGraph() {\n      this.isLoading = true\n      let baseUrl = `/devices/${this.$route.params.hostname}/graphs/health/device_uptime?legend=no`\n      let fromDate = `from=${toTimestamp(this.dateStart)}`\n      let toDate = `to=${toTimestamp(this.dateEnd)}`\n      let width = `width=${500}`\n      let height = `height=${160}`\n      let url = [baseUrl, fromDate, toDate, width, height].join(\"&\")\n\n      this.axios\n        .get(url, { responseType: \"blob\" })\n        .then((response) => {\n          if (response.data.size === 0) {\n            this.isEmpty = true\n            this.isLoading = false\n          } else {\n            let reader = new FileReader()\n\n            reader.readAsDataURL(response.data)\n\n            reader.onload = () => {\n              this.uptimeImage = reader.result\n            }\n\n            this.isEmpty = false\n            this.isLoading = false\n          }\n        })\n        .catch((e) => {\n          this.errors.push(e)\n          this.isLoading = false\n        })\n    },\n  },\n}\n</script>\n\n"],"sourceRoot":"src/components"}]}
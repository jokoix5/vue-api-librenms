o\">\n          <v-expansion-panel>\n            <v-expansion-panel-header expand-icon=\"mdi-menu-down\">\n              Information\n            </v-expansion-panel-header>\n\n            <v-expansion-panel-content>\n              <p class=\"display-1 text--primary\">{{ device.sysName }}</p>\n              <v-divider></v-divider>\n\n              <v-simple-table class=\"mt-2\">\n                <tbody>\n                  <tr class=\"my-2\">\n                    <td width=\"30%\"><strong>IP Address</strong></td>\n                    <td class=\"text-right\">{{ device.hostname }}</td>\n                  </tr>\n\n                  <tr>\n                    <td width=\"30%\"><strong>Hardware</strong></td>\n                    <td class=\"text-right\">{{ device.hardware }}</td>\n                  </tr>\n\n                  <tr class=\"my-2\">\n                    <td width=\"30%\"><strong>Operating System</strong></td>\n                    <td class=\"text-right\">{{ device.os }}</td>\n                  </tr>\n\n                  <tr class=\"my-2\">\n                    <td width=\"30%\"><strong>Object ID</strong></td>\n                    <td class=\"text-right\">{{ device.sysObjectID }}</td>\n                  </tr>\n\n                  <tr class=\"my-2\">\n                    <td width=\"30%\"><strong>Contact</strong></td>\n                    <td class=\"text-right\">{{ device.sysContact }}</td>\n                  </tr>\n\n                  <tr class=\"my-2\">\n                    <td width=\"30%\"><strong>Uptime</strong></td>\n                    <td class=\"text-right\">{{ timeCounter(device.uptime) }}</td>\n                  </tr>\n\n                  <tr class=\"my-2\">\n                    <td width=\"30%\"><strong>Location</strong></td>\n                    <td class=\"text-right\">{{ device.location }}</td>\n                  </tr>\n\n                  <tr class=\"my-2\">\n                    <td width=\"30%\"><strong>Lat/Lng</strong></td>\n                    <td class=\"text-right\">\n                      {{ device.lat }} {{ device.lng }}\n                    </td>\n                  </tr>\n                </tbody>\n              </v-simple-table>\n            </v-expansion-panel-content>\n          </v-expansion-panel>\n        </v-expansion-panels>\n      </v-col>\n\n      <v-col\n        cols=\"12\"\n        md=\"8\"\n        lg=\"8\"\n        offset-md=\"4\"\n        offset-lg=\"4\"\n        class=\"my-2 px-2\"\n      >\n        <v-expansion-panels flat v-model=\"sysUptime\">\n          <v-expansion-panel>\n            <v-expansion-panel-header expand-icon=\"mdi-menu-down\">\n              Uptime\n            </v-expansion-panel-header>\n\n            <v-expansion-panel-content>\n              <Uptime :dateStart=\"dateStart\" :dateEnd=\"dateEnd\" />\n            </v-expansion-panel-content>\n          </v-expansion-panel>\n        </v-expansion-panels>\n\n        <v-expansion-panels flat v-model=\"sysTemp\" class=\"my-2\">\n          <v-expansion-panel>\n            <v-expansion-panel-header expand-icon=\"mdi-menu-down\">\n              Temperature\n            </v-expansion-panel-header>\n\n            <v-expansion-panel-content>\n              <Temperature />\n            </v-expansion-panel-content>\n          </v-expansion-panel>\n        </v-expansion-panels>\n\n        <v-expansion-panels flat v-model=\"sysPro\" class=\"my-2\">\n          <v-expansion-panel>\n            <v-expansion-panel-header expand-icon=\"mdi-menu-down\">\n              Processor\n            </v-expansion-panel-header>\n\n            <v-expansion-panel-content>\n              <v-card flat>\n                <Processor :dateStart=\"dateStart\" :dateEnd=\"dateEnd\" />\n              </v-card>\n            </v-expansion-panel-content>\n          </v-expansion-panel>\n        </v-expansion-panels>\n\n        <v-expansion-panels flat v-model=\"sysMem\" class=\"my-2\">\n          <v-expansion-panel>\n            <v-expansion-panel-header expand-icon=\"mdi-menu-down\">\n              Memory\n            </v-expansion-panel-header>\n\n            <v-expansion-panel-content>\n              <v-card flat>\n                <Memory :dateStart=\"dateStart\" :dateEnd=\"dateEnd\" />\n              </v-card>\n            </v-expansion-panel-content>\n          </v-expansion-panel>\n        </v-expansion-panels>\n\n        <v-expansion-panels flat v-model=\"sysStog\" class=\"my-2\">\n          <v-expansion-panel>\n            <v-expansion-panel-header expand-icon=\"mdi-menu-down\">\n              Storage\n            </v-expansion-panel-header>\n\n            <v-expansion-panel-content>\n              <v-card flat>\n                <Storage :dateStart=\"dateStart\" :dateEnd=\"dateEnd\" />\n              </v-card>\n            </v-expansion-panel-content>\n          </v-expansion-panel>\n        </v-expansion-panels>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport DateTime from \"@/components/Datetime\"\nimport Uptime from \"@/components/Uptime\"\nimport Temperature from \"@/components/Temperature\"\nimport Storage from \"@/components/Storage\"\nimport Memory from \"@/components/Memory\"\nimport Processor from \"@/components/Processor\"\n\n\nexport default {\n  name: \"Detail\",\n\n  components: {\n    Uptime,\n    Temperature,\n    Storage,\n    Memory,\n    Processor,\n    DateTime,\n  },\n\n  data: () => ({\n    device: {},\n    dateStart: \"\",\n    dateEnd: \"\",\n    sysUptime: 0,\n    sysInfo: 0,\n    sysTemp: 0,\n    sysStog: 0,\n    sysMem: 0,\n    sysPro: 0,\n    // timer: '',\n  }),\n  // created () {\n  //   this.timer = setInterval(this.fetchEventsList, 300000)\n  // },\n  mounted() {\n    this.initDateStart()\n    this.initDateEnd()\n    this.fetchDevice()\n    // this.timer = setInterval(this.fetchDevice, 300000)\n  },\n\n  methods: {\n    initDateStart() {\n      let start = new Date()\n      start.setHours(0, 0, 0, 0)\n      this.dateStart = start\n    },\n\n    initDateEnd() {\n      let end = new Date()\n      end.setHours(23, 59, 59, 999)\n      this.dateEnd = end\n    },\n\n    fetchDevice() {\n      this.axios\n        .get(`/devices/${this.$route.params.hostname}`)\n        .then((response) => {\n          this.device = response.data.devices[0]\n        })\n        .catch((e) => {\n          this.errors.push(e)\n        })\n    },\n\n    handleDateTime(value) {\n      const { dateStart, dateEnd } = value\n\n      this.dateStart = dateStart\n      this.dateEnd = dateEnd\n    },\n\n    timeCounter(item) {\n      var content = \"\"\n      var t = item\n\n      var days = parseInt(t / 86400)\n      t = t - days * 86400\n\n      var hours = parseInt(t / 3600)\n      t = t - hours * 3600\n\n      var minutes = parseInt(t / 60)\n      t = t - minutes * 60\n\n      if (days) content += days + \" days\"\n      if (hours || days) {\n        if (days) content += \", \"\n        content += hours + \" hours, \"\n      }\n\n      content += minutes + \" minutes and \" + t + \" seconds\"\n\n      return content\n    },\n    // cancelAutoUpdate () { clearInterval(this.timer) }\n  },\n  // beforeDestroy () {\n  //     clearInterval(this.timer)\n  //   }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.fixed-details {\n  position: fixed;\n}\n\n@media only screen and (max-device-width: 960px) {\n  .fixed-details {\n    position: relative;\n  }\n}\n</style>\n\n"],"sourceRoot":"src/views"}]}
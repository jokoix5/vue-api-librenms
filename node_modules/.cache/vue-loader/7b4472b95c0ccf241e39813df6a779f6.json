: () => ({\n    devices: {}\n  }),\n\n  mounted() {\n    this.fetchDevices();\n  },\n\n  methods: {\n    fetchDevices() {\n      this.axios\n        .get(\"/devices\")\n        .then((response) => {\n          let data = response.data.devices;\n          this.groupingDevices(data);\n        })\n        .catch((e) => {\n          this.errors.push(e);\n        });\n    },\n\n    groupingDevices(devices) {\n      let _ = require(\"lodash\");\n      let result = _.groupBy(devices, function(n) {\n        return n.type;\n      });\n\n      this.devices = result;\n    }\n  }\n};\n</script>\n\n"]}]}